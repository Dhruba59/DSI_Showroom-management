package one;

public class sportsVehicle extends vehicle {

	public sportsVehicle(int id) {	
		
		super(id);
		super.setEngineType(new String[] {"oil"});
		super.setTurbo(true);
		super.setType("sports");
		super.setEnginePower(2500);
		super.setTireSize(250);
		super.setModelNo("SPORTSDEMO101");
	}
}
